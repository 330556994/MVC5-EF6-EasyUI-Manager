    function initDefault() {
        $('#swText0').switchbutton({
            onChange: function(checked) {
                if (checked) {
                    $('#swImage0').switchbutton("uncheck");
                    $('#swVoice0').switchbutton("uncheck");
                    $("#div01").show();
                    $("#div02,#div03").hide();
                    $("#Category").val(Category.Text);
                }
            }
        });
        $('#swImage0').switchbutton({
            onChange: function(checked) {
                if (checked) {
                    $('#swVoice0').switchbutton("uncheck");
